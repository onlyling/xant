{
  "name": "xant",
  "version": "0.0.1",
  "private": true,
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.ts",
  "types": "lib/typescript/index.d.ts",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint '{components,demo}/**/*.{js,jsx,ts,tsx}' --fix",
    "prettier": "prettier --write '{demo,components}/**/*.{js,json,yml,yaml,ts,tsx,md}'",
    "lint-staged": "lint-staged --allow-empty",
    "commit": "npx git-cz",
    "ncu": "npx npm-check-updates -u -x react,react-native,react-test-renderer",
    "doc": "dumi dev",
    "doc:build": "dumi build",
    "doc:commit": "git add docs-dist && git commit -n -m 'commit doc file'",
    "doc:pulish-safe": "git subtree push --prefix docs-dist origin gh-pages",
    "doc:pulish": "git push origin `git subtree split --prefix docs-dist master`:gh-pages --force",
    "doc:reset": "git reset --hard HEAD^",
    "build": "bob build",
    "prepare": "husky install"
  },
  "dependencies": {
    "omit.js": "^2.0.2",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-svg": "^12.1.1"
  },
  "resolutions": {
    "@types/react": "^17",
    "prettier": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/runtime": "^7.14.8",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@react-navigation/bottom-tabs": "^6.0.1",
    "@react-navigation/native": "^6.0.1",
    "@react-navigation/stack": "^6.0.1",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.64.12",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.16.0",
    "dumi": "^1.1.24",
    "dumi-theme-mobile": "^1.1.7",
    "eslint": "^7.32.0",
    "git-cz": "^4.7.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-builder-bob": "^0.18.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-screens": "^3.5.0",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^27.0.4",
    "typescript": "4.3.5",
    "umi-plugin-react-native": "^0.5.0"
  },
  "lint-staged": {
    "*.{yml,yaml,md}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "bash -c tsc --noEmit",
      "eslint --fix",
      "prettier --write"
    ]
  },
  "react-native-builder-bob": {
    "source": "components",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
