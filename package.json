{
  "name": "xant",
  "version": "0.0.1",
  "private": true,
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "commit": "npx git-cz",
    "ncu": "npx npm-check-updates -u -x react,react-native,react-test-renderer",
    "doc": "dumi dev",
    "doc-build": "dumi build",
    "doc-commit": "git add docs-dist && git commit -m 'update doc'",
    "doc-pulish": "git subtree push --prefix docs-dist origin gh-pages -f",
    "clean": "antd-tools run clean",
    "compile": "npm run clean && antd-tools run compile"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "core-js": "^3.11.1",
    "omit.js": "^2.0.2",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.1.1",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@ant-design/tools": "^13.4.0",
    "@babel/core": "^7.14.0",
    "@babel/runtime": "^7.14.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "dumi": "^1.1.16",
    "dumi-theme-mobile": "^1.0.16",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "umi-plugin-react-native": "^0.4.0"
  }
}
